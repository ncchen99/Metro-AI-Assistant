{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "api/index.py": {
            "runtime": "python3.9"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "OPENAI_API_KEY": "@openai_api_key"
    }
}